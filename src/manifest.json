{

  "_notes_": "keys starting with '_notes_' will be removed by grunt",
  "_notes_package.json": "where keys overlap, they are taken from package.json",
  "_notes_browsers": "browser specific keys and overrides are taken from: manifest.<BROWSER>.json",

  "manifest_version": 2,
  "description": "__MSG_extension_description__",
  "default_locale": "en",
  "incognito": "split",
  "icons": {
    "19": "img/19.png",
    "20": "img/20.png",
    "30": "img/30.png",
    "35": "img/35.png",
    "38": "img/38.png",
    "40": "img/40.png",
    "48": "img/48.png",
    "96": "img/96.png"
  },
  "permissions": [
    "http://*/.well-known/security.txt",
    "http://*/security.txt",
    "http://*/humans.txt",
    "https://*/.well-known/security.txt",
    "https://*/security.txt",
    "https://*/humans.txt",
    "tabs",
    "storage"
  ],
  "page_action": {},
  "background": {
    "persistent": false
  }
}